  "bugs": {
    "url": "https://github.com/amberlovescats14/RunningConcurrent/issues"
  },
  "homepage": "https://github.com/amberlovescats14/RunningConcurrent#readme",
  "dependencies": {
    "concurrently": "^4.1.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
